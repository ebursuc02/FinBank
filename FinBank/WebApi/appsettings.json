{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "FinBank": "Server=localhost\\SQLEXPRESS;Database=FinBank;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "Ef": {
    "CommandTimeoutSeconds": 60,
    "EnableRetryOnFailure": false,
    "MaxRetryCount": 5,
    "MaxRetryDelaySeconds": 30,
    "UseDetailedErrors": false,
    "UseSensitiveDataLogging": false
  },
  
  "Jwt": {
    "Key": "a_very_long_secret_key_at_least_32_chars",
    "Issuer": "MyApi",
    "Audience": "MyApiClients",
    "ExpiryMinutes": 60
  }
}
